{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug ConfigSafe Auditor",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/configsafe_auditor.exe",
      "args": ["-f", "sample.conf"],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "C:/msys64/ucrt64/bin/gdb.exe",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "Build ConfigSafe Auditor"
    }
  ]
}